
public class ControleBonificacao {
	
	private double soma;
	
	//mesmo usando uma referencia geral, sempre será chamado um método específico de cada classe filha dessa referencia geral
	public void registra(Funcionario f) {
		//
		double boni =  f.getBonificacao();
		this.soma = this.soma + boni;
	}
	
	public double getSoma() {
		return soma;
	}
}
