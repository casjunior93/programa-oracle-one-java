
public class ControleBonificacao {
	
	private double soma;
	
	//mesmo usando uma referencia geral, sempre será chamado um método específico da classe para onde a referencia do objeto aponta
	public void registra(Funcionario f) {
		//
		double boni =  f.getBonificacao();
		this.soma = this.soma + boni;
	}
	
	public double getSoma() {
		return soma;
	}
}
