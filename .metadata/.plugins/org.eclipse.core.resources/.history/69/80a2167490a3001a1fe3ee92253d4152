//extendo a conta vc herda os atributos e métodos, mas não herda os construtores
//por isso deve-se escrever o construtor
public class ContaCorrente extends Conta {
	
	//construtor
	public ContaCorrente(int agencia, int numero) {
		//o construtor chama automaticamente o construtor da classe mãe
		//é implicito
		super(agencia, numero);
	}
	
}
