
public class testeFuncionario {
	public static void main(String[] args) {
		//cria um funcionario chamando um construtor
		//insere construtor padrão se não for criado um em Funcionario.java
		//referencia criada
		Funcionario carlos = new Funcionario();
		
		carlos.setNome("Carlos");
		carlos.setCpf("419.415.368-59");
		carlos.setSalario(1350);
		
		System.out.println("Nome: " + carlos.getNome());
		System.out.println("Cpf: " + carlos.getCpf());
		System.out.println("Salário: " + carlos.getSalario());
		System.out.println("Bonificação: " + carlos.getBonificacao());
	}
}
